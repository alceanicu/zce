{
  "manifest_version": 2,
  "name": "ZCE",
  "short_name": "ZCE-PHP ZEND",
  "version": "2.0.3",
  "author": "Nicu ALCEA",
  "description": "ZCE - PHP ZEND Certification Exam Simulator",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "incognito": "spanning",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';",
  "web_accessible_resources": [
    "assets/css/*",
    "assets/img/*",
    "assets/sass/*"
  ],
  "browser_action": {
    "default_icon": {
      "48": "assets/icons/php48.png"
    },
    "default_title": "ZEND exam"
  },
  "homepage_url": "https://github.com/alceanicu/zce"
}

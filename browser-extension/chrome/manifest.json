{
  "manifest_version": 2,
  "name": "ZCE simulator",
  "short_name": "ZCE-PHP ZEND",
  "version": "1.0.1",
  "version_name": "1.0.1 - RC2",
  "author": "Nicu ALCEA",
  "description": "ZCE - PHP ZEND Certification exam simulator",
  "minimum_chrome_version": "22",
  "icons": {
    "16": "assets/icons/php16.png",
    "32": "assets/icons/php32.png",
    "38": "assets/icons/php38.png",
    "48": "assets/icons/php48.png",
    "128": "assets/icons/php128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "incognito": "spanning",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';",
  "web_accessible_resources": [
    "assets/css/*",
    "assets/img/*",
    "assets/sass/*"
  ],
  "browser_action": {
    "default_icon": "assets/icons/php32.png",
    "default_title": "ZCE - PHP ZEND Certification exam simulator"
  },
  "homepage_url": "https://github.com/alceanicu/zce",
  "permissions": [
    "<all_urls>",
    "tabs",
    "activeTab"
  ]
}
